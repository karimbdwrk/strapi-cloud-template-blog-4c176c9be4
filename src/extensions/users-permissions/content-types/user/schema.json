{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "isDoctor": {
      "type": "boolean",
      "default": false
    },
    "UUID": {
      "type": "customField",
      "options": {
        "uuid-format": "^[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}$"
      },
      "customField": "plugin::strapi-advanced-uuid.uuid"
    },
    "phone": {
      "type": "string",
      "regex": "^\\d+$"
    },
    "doctor": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::doctor.doctor",
      "mappedBy": "users_permissions_user"
    },
    "appointments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::appointment.appointment",
      "mappedBy": "patient"
    },
    "ratings": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::rating.rating",
      "mappedBy": "users_permissions_user"
    },
    "user_record": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::user-record.user-record",
      "mappedBy": "users_permissions_user"
    },
    "user_information": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::userinformation.userinformation",
      "inversedBy": "user"
    },
    "firstname": {
      "type": "string"
    },
    "lastname": {
      "type": "string"
    }
  }
}
